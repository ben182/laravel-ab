{
    "name": "ben182/laravel-ab",
    "description": "Laravel A/B experiment testing tool",
    "keywords": [
        "ben182",
        "php",
        "laravel",
        "laravel-package",
        "laravel-5-package",
        "ab-testing"
    ],
    "homepage": "https://github.com/ben182/laravel-ab",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ben",
            "email": "moin@benjaminbortels.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "illuminate/support": "^6.0 | ^7.0 | ^8.0",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.9.* | ^6.0",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ben182\\AbTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ben182\\AbTesting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ben182\\AbTesting\\AbTestingServiceProvider"
            ],
            "aliases": {
                "AbTesting": "Ben182\\AbTesting\\AbTestingFacade"
            }
        }
    }
}
